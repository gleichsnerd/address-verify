{
  "name": "address-verify",
  "version": "1.0.0",
  "description": "A nicely robust implementation of an address verification cli tool in Node",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup": "chmod +x address-verify && chmod +x setup.sh && ./setup.sh",
    "build": "tsc --sourceMap",
    "start": "node dist/index.js",
    "start:dev": "npm run build && npm run start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "author": "-redacted-",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^20.8.0",
    "@types/smartystreets-javascript-sdk": "^5.1.0",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "prettier": "3.5.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "fast-csv": "^5.0.2",
    "smartystreets-javascript-sdk": "^6.3.0",
    "yargs": "^17.7.2"
  }
}
